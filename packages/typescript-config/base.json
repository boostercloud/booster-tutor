{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@sindresorhus/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "incremental": false,
    "moduleResolution": "node",
    "isolatedModules": true,
    "lib": ["es2020", "DOM", "DOM.Iterable"],
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2020"
  }
}
